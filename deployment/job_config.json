{
  "name": "AIS Pipelines Dripper",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "dripper_task",
      "python_wheel_task": null,
      "notebook_task": null,
      "spark_python_task": {
        "python_file": "dbfs:/FileStore/scripts/dripper.py",
        "parameters": []
      },
      "libraries": [],
      "timeout_seconds": 0,
      "max_retries": 0,
      "min_retry_interval_millis": 0,
      "retry_on_timeout": false,
      "email_notifications": {}
    }
  ],
  "job_clusters": [],
  "format": "MULTI_TASK",
  "queue": {
    "enabled": false
  },
  "parameters": [
    {
      "name": "CAT",
      "default": "main"
    },
    {
      "name": "SCH",
      "default": "streaming"
    },
    {
      "name": "SRC_VOL",
      "default": "full_history"
    },
    {
      "name": "DST_VOL",
      "default": "landing"
    },
    {
      "name": "N_PER_RUN",
      "default": "5"
    }
  ],
  "schedule": {
    "quartz_cron_expression": "0 * * * * ?",
    "timezone_id": "UTC",
    "pause_status": "UNPAUSED"
  },
  "compute": [
    {
      "compute_key": "default",
      "spec": {
        "kind": "serverless_compute"
      }
    }
  ]
}
